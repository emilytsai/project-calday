<div class="container">
	<h2>Profile</h2>
	<p>Name: <%= current_student.name if current_student.name %></p>
	<p>Email: <%= current_student.email if current_student.email %></p>
  <p>Year: <%= current_student.year if current_student.year %></p>
  <p>Major: <%= current_student.major if current_student.major %></p>
  <p>SID: <%= current_student.SID if current_student.SID %></p>
  <p><%= button_to "Edit Profile", edit_student_registration_path, :class => "btn btn-lg btn-default", :method => :get %></p>
</div>

</body>
